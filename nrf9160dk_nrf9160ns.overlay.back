&uart0 {
	compatible = "nordic,nrf-uarte";
	status = "okay";
};

&uart1 {
	compatible = "nordic,nrf-uarte";
	status = "okay";
	current-speed = <115200>;
	hw-flow-control;
	rx-pin = <17>; /* <&interface_to_nrf52840 0 0>; */
	tx-pin = <18>; /* <&interface_to_nrf52840 1 0>; */
	cts-pin = <19>; /* <&interface_to_nrf52840 2 0>; */
	rts-pin = <21>; /* <&interface_to_nrf52840 3 0>; */
};
